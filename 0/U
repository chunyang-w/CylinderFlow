FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        uniformValue    table ((0 (1 0 0)) (5 (10 0 0)));
    }

    outlet
    {
        type            zeroGradient;
    }

    cylinder
    {
        type            rotatingWallVelocity;
        origin          (0 0 0);
        axis            (0 0 1);
        rpm             500;
    }

    walls
    {
        type            noSlip;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
